.container{
  width: 100%;
  height: 500px;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  
}
.photo_view{
  width: 100%;
  height: 210px;
  /* background-color: burlywood; */
  display: flex;
  justify-content: space-evenly
  
}

.photo {
  width: 35%;
  height: 100%;

  /* background-color: powderblue; */
  /* border:1px solid #51A938; */
